{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES6",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "declaration": true,
    "resolveJsonModule": true,
    "isolatedDeclarations": true,
    "types": [
      "node"
    ],
    "paths": {
      "unenv/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}
