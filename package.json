{
  "name": "unenv",
  "version": "1.2.1",
  "description": "",
  "repository": "unjs/unenv",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./runtime/mock/proxy-cjs": "./runtime/mock/proxy.cjs",
    "./runtime/mock/proxy-cjs/*": "./runtime/mock/proxy.cjs",
    "./runtime/*": {
      "require": "./runtime/*.cjs",
      "types": "./runtime/*.d.ts",
      "import": "./runtime/*.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "runtime"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint --ext .ts . && prettier -c src",
    "typecheck": "tsc --noEmit",
    "prepack": "unbuild",
    "release": "pnpm test && changelogen --release && pnpm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm typecheck"
  },
  "dependencies": {
    "defu": "^6.1.2",
    "mime": "^3.0.0",
    "node-fetch-native": "^1.0.2",
    "pathe": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "changelogen": "^0.5.1",
    "consola": "^2.15.3",
    "eslint": "^8.35.0",
    "eslint-config-unjs": "^0.1.0",
    "jiti": "^1.17.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2"
  },
  "packageManager": "pnpm@7.29.1"
}
